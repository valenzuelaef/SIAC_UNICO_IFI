                                <td class="col-sm-6"><label id="lblNacionalidad"></label></td>
                            </tr>
                            <tr>
                                <td class="col-sm-6"><label class="control-label">Fecha de Nacimiento:</label></td>
                                <td class="col-sm-6"><label id="lblFechaNac"></label></td>
                            </tr> 
                            <tr>
                                <td class="col-sm-6"><label class="control-label">Estado Civil:</label></td>
                                <td class="col-sm-6"><label id="lblEstadoCiv"></label></td>
