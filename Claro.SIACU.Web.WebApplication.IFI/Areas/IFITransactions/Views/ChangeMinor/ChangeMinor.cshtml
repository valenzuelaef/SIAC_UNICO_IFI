                                                    <div class="col-sm-3">
                                                        <select class="form-control select-list" id="cboProvinciaMod" name="nmcboProvinciaMod"></select>
                                                    </div>

                                                    <div class="col-sm-3">
                                                        <label for="idMovil">Distrito:</label>
