                            <tr>
                                <td class="col-sm-6"><label class="control-label">Contacto Cliente:</label></td>
                                <td class="col-sm-6"><label id="lblContactoCli"></label></td>
                            </tr> 
                            <tr>
                                <td class="col-sm-6"><label class="control-label">Teléfono Contacto:</label></td>
                                <td class="col-sm-6"><label id="lblTelefono"></label></td>
                            </tr>
                            <tr>
                                <td class="col-sm-6"><label class="control-label">Email de contacto:</label></td>
                                <td class="col-sm-6"><label id="lblEmail"></label></td>
                            </tr> 
                            <tr>
                                <td class="col-sm-6"><label class="control-label">Número Fax:</label></td>
                                <td class="col-sm-6"><label id="lblFax"></label></td>
                            </tr>                                                    
                                      
                            </tbody>
                        </table>
                       
                    </div>
                    <br />
                    <br />
                    <div id="businessRulesHead" role="tab" class="panel-heading">
                        <span class="glyphicon glyphicon-th-list"></span>
